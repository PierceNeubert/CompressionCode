    elif item == '011100':
        newlist.append('.')
    elif item == '011101':
        newlist.append(',')
    elif item == '011110':
        newlist.append("-")
    elif item == '011111':
        newlist.append("'")
    elif item == '100000':
        newlist.append('"')
    elif item == '100001':
