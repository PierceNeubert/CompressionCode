import ECS101code1
binary_str = ECS101code1.com_str
bin_split = []
n = 6
for i in range(0, len(binary_str), n):
    bin_split.append(binary_str[i : i + n])
print(bin_split)

newlist = []
for item in bin_split:
    if item == '000000':
        newlist.append(' ')
    elif item == '000001':
        newlist.append('a')
    elif item == '000010':
        newlist.append('b')
    elif item == '000011':
        newlist.append('c')
    elif item == '000100':
